        var adBlockDetected = function() {
            $('h1 span').text('yes');
        }
        // We create the function that will be executed if AdBlock is NOT detected
        var adBlockUndetected = function() {
            $('h1 span').text('no');
        }
        // We observe if the variable "fuckAdBlock" exists
